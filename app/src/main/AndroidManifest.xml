<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="prisioners.day.hacker.science.hackerday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <uses-permission android:name="android.permission.INTERNET" />
        <service android:name=".WaitForFriend" />
        <activity android:name=".Start"/>
        <activity android:name=".Find"/>
        <activity android:name=".Game"/>
        <activity android:name=".Question1"/>
        <activity android:name=".End"/>
        <activity android:name=".Question2"/>
        <activity android:name=".Question3"/>
        <activity android:name=".Question4"/>
    </application>

</manifest>